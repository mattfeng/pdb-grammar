{
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "source.pdb markup.header",
        "settings": {
          "foreground": "#ffb477"
        }
      },
      {
        "scope": "source.pdb markup.header.idcode",
        "settings": {
          "foreground": "#ff9277"
        }
      },
      {
        "scope": "source.pdb markup.title",
        "settings": {
          "foreground": "#ffd477"
        }
      },
      {
        "scope": "source.pdb comment.compound",
        "settings": {
          "foreground": "#979797"
        }
      },
      {
        "scope": "source.pdb comment.source",
        "settings": {
          "foreground": "#777777"
        }
      },
      {
        "scope": "source.pdb comment.keywords",
        "settings": {
          "foreground": "#c8ffbd"
        }
      },
      {
        "scope": "source.pdb comment.experimentdata",
        "settings": {
          "foreground": "#bdffe3"
        }
      },
      {
        "scope": "source.pdb comment.line.remark",
        "settings": {
          "foreground": "#3f3f3f"
        }
      },
      {
        "scope": "source.pdb comment.line.number",
        "settings": {
          "foreground": "#6d6d6d"
        }
      },
      {
        "scope": "source.pdb entity.atom",         // entire ATOM line
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "scope": "source.pdb entity.atom.keyword", // ATOM keyword
        "settings": {
          "foreground": "#89b8ff"
        }
      },
      {
        "scope": "source.pdb entity.atom.coords", // ATOM coordinates
        "settings": {
          "foreground": "#ffd29f"
        }
      },
    ]
  }
}